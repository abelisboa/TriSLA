{
  "dashboard": {
    "title": "TriSLA - Module Metrics",
    "tags": ["trisla", "modules", "metrics"],
    "timezone": "browser",
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Intents Processados",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_intents_total[5m])",
            "legendFormat": "Intents/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "NESTs Gerados",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_nests_generated_total[5m])",
            "legendFormat": "NESTs/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Predições ML",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_predictions_total[5m])",
            "legendFormat": "Predictions/sec"
          }
        ]
      },
      {
        "id": 4,
        "title": "Duração de Predições",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(trisla_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(trisla_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(trisla_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Duration (ms)"
          }
        ]
      },
      {
        "id": 5,
        "title": "Decisões",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_decisions_total[5m])",
            "legendFormat": "Decisions/sec"
          }
        ]
      },
      {
        "id": 6,
        "title": "Latência de Decisões",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(trisla_decision_latency_seconds_bucket[5m]))",
            "legendFormat": "p99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(trisla_decision_latency_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(trisla_decision_latency_seconds_bucket[5m]))",
            "legendFormat": "p50"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency (ms)"
          }
        ]
      },
      {
        "id": 7,
        "title": "Transações Blockchain",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_blockchain_transactions_total[5m])",
            "legendFormat": "Transactions/sec"
          }
        ]
      },
      {
        "id": 8,
        "title": "Duração de Transações",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(trisla_transaction_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Duration (ms)"
          }
        ]
      },
      {
        "id": 9,
        "title": "Ações Executadas",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trisla_actions_executed_total[5m])",
            "legendFormat": "Actions/sec"
          }
        ]
      },
      {
        "id": 10,
        "title": "Duração de Ações",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(trisla_action_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Duration (ms)"
          }
        ]
      }
    ]
  }
}

