<?xml version="1.0"?>
<rdf:RDF xmlns="http://trisla.org/ontology#"
     xml:base="http://trisla.org/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://trisla.org/ontology">
        <rdfs:comment xml:lang="en">TriSLA Ontology for Network Slice Management with SLA Guarantee - Complete OWL version 3.6.0</rdfs:comment>
        <owl:versionInfo>3.6.0</owl:versionInfo>
    </owl:Ontology>

    <!-- 
    ============================================
    CLASSES PRINCIPAIS
    ============================================
    -->

    <!-- Intent Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#Intent">
        <rdfs:comment>Classe base para intenções de serviço</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#UseCaseIntent">
        <rdfs:comment>Intenção baseada em caso de uso específico</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Intent"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#SliceRequest">
        <rdfs:comment>Requisição de criação de network slice</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <!-- Slice Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#Slice">
        <rdfs:comment>Classe base para network slice</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#SliceType">
        <rdfs:comment>Tipo de network slice conforme 3GPP</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#eMBB_Slice">
        <rdfs:comment>Enhanced Mobile Broadband slice</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Slice"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#URLLC_Slice">
        <rdfs:comment>Ultra-Reliable Low-Latency Communications slice</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Slice"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#mMTC_Slice">
        <rdfs:comment>massive Machine-Type Communications slice</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Slice"/>
    </owl:Class>

    <!-- SLA Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#SLA">
        <rdfs:comment>Service Level Agreement</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#SLO">
        <rdfs:comment>Service Level Objective</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#SLI">
        <rdfs:comment>Service Level Indicator</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <!-- Metric Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#Metric">
        <rdfs:comment>Métrica de performance</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#LatencyMetric">
        <rdfs:comment>Métrica de latência</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Metric"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#ThroughputMetric">
        <rdfs:comment>Métrica de throughput</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Metric"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#ReliabilityMetric">
        <rdfs:comment>Métrica de confiabilidade</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://trisla.org/ontology#Metric"/>
    </owl:Class>

    <!-- 3GPP TS 28.541 Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#qosCharacteristics">
        <rdfs:comment>QoS Characteristics conforme 3GPP TS 28.541</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#sliceProfile">
        <rdfs:comment>Slice Profile conforme 3GPP TS 28.541</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#coverageArea">
        <rdfs:comment>Área de cobertura conforme 3GPP</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <!-- Template Classes -->
    <owl:Class rdf:about="http://trisla.org/ontology#GSTTemplate">
        <rdfs:comment>Generic Slice Template conforme 3GPP</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <owl:Class rdf:about="http://trisla.org/ontology#NESTTemplate">
        <rdfs:comment>Network Slice Template conforme 3GPP TS 28.541</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>

    <!-- 
    ============================================
    OBJECT PROPERTIES
    ============================================
    -->

    <owl:ObjectProperty rdf:about="http://trisla.org/ontology#hasSliceType">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Intent"/>
        <rdfs:range rdf:resource="http://trisla.org/ontology#SliceType"/>
        <rdfs:comment>Relaciona intent com tipo de slice</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://trisla.org/ontology#hasSLA">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://trisla.org/ontology#SLA"/>
        <rdfs:comment>Relaciona slice com SLA</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://trisla.org/ontology#hasQoSCharacteristics">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:comment>Slice tem características QoS</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://trisla.org/ontology#hasSliceProfile">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://trisla.org/ontology#sliceProfile"/>
        <rdfs:comment>Slice tem perfil</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://trisla.org/ontology#hasCoverageArea">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://trisla.org/ontology#coverageArea"/>
        <rdfs:comment>Slice tem área de cobertura</rdfs:comment>
    </owl:ObjectProperty>

    <!-- 
    ============================================
    DATA PROPERTIES - 3GPP TS 28.541 Complete
    ============================================
    -->

    <!-- QoS Characteristics Properties -->
    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#priorityLevel">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Priority Level (1-8 conforme 3GPP)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#packetDelayBudget">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Packet Delay Budget em ms</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#packetErrorRate">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Packet Error Rate (0-1)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#guaranteedBitRate">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Guaranteed Bit Rate em Mbps</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#maximumBitRate">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#qosCharacteristics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Maximum Bit Rate em Mbps</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Slice Profile Properties -->
    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#plmnId">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#sliceProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>PLMN Identifier</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#sNSSAI">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#sliceProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Single Network Slice Selection Assistance Information</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#maxNumberOfUEs">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#sliceProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Número máximo de UEs</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#maxDataRatePerUE">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#sliceProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Taxa máxima de dados por UE em Mbps</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Coverage Area Properties -->
    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#areaType">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#coverageArea"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Tipo de área (Urban, Rural, Suburban)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#geographicCoordinates">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#coverageArea"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Coordenadas geográficas (lat,lon)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Basic SLA Properties -->
    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#hasLatency">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Latência máxima em milissegundos</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#hasThroughput">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Throughput mínimo em Mbps</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://trisla.org/ontology#hasReliability">
        <rdfs:domain rdf:resource="http://trisla.org/ontology#Slice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Confiabilidade (0-1)</rdfs:comment>
    </owl:DatatypeProperty>

</rdf:RDF>


































