@prefix : <http://trisla.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TriSLAOntology a owl:Ontology ;
    rdfs:comment "Ontologia TriSLA para gerenciamento de Network Slices com garantia de SLA" ;
    owl:versionInfo "3.5.0" .

# ============================================
# CLASSES PRINCIPAIS
# ============================================

# Intent e UseCaseIntent
:Intent a owl:Class ;
    rdfs:comment "Classe base para intenções de serviço" ;
    rdfs:subClassOf owl:Thing .

:UseCaseIntent a owl:Class ;
    rdfs:comment "Intenção baseada em caso de uso específico" ;
    rdfs:subClassOf :Intent .

:SliceRequest a owl:Class ;
    rdfs:comment "Requisição de criação de network slice" ;
    rdfs:subClassOf owl:Thing .

# Slice e tipos
:Slice a owl:Class ;
    rdfs:comment "Classe base para network slice" ;
    rdfs:subClassOf owl:Thing .

:SliceType a owl:Class ;
    rdfs:comment "Tipo de network slice conforme 3GPP" ;
    rdfs:subClassOf owl:Thing .

:eMBB_Slice a owl:Class ;
    rdfs:comment "Enhanced Mobile Broadband slice" ;
    rdfs:subClassOf :Slice .

:URLLC_Slice a owl:Class ;
    rdfs:comment "Ultra-Reliable Low-Latency Communications slice" ;
    rdfs:subClassOf :Slice .

:mMTC_Slice a owl:Class ;
    rdfs:comment "massive Machine-Type Communications slice" ;
    rdfs:subClassOf :Slice .

:UseCaseSlice a owl:Class ;
    rdfs:comment "Slice baseado em caso de uso específico" ;
    rdfs:subClassOf :Slice .

# SLA, SLO, SLI, Metric
:SLA a owl:Class ;
    rdfs:comment "Service Level Agreement" ;
    rdfs:subClassOf owl:Thing .

:SLO a owl:Class ;
    rdfs:comment "Service Level Objective" ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :belongsToSLA ;
        owl:someValuesFrom :SLA
    ] .

:SLI a owl:Class ;
    rdfs:comment "Service Level Indicator" ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :measuresSLO ;
        owl:someValuesFrom :SLO
    ] .

:Metric a owl:Class ;
    rdfs:comment "Métrica de performance" ;
    rdfs:subClassOf owl:Thing .

:LatencyMetric a owl:Class ;
    rdfs:comment "Métrica de latência" ;
    rdfs:subClassOf :Metric .

:ThroughputMetric a owl:Class ;
    rdfs:comment "Métrica de throughput" ;
    rdfs:subClassOf :Metric .

:ReliabilityMetric a owl:Class ;
    rdfs:comment "Métrica de confiabilidade" ;
    rdfs:subClassOf :Metric .

:JitterMetric a owl:Class ;
    rdfs:comment "Métrica de jitter" ;
    rdfs:subClassOf :Metric .

:PacketLossMetric a owl:Class ;
    rdfs:comment "Métrica de perda de pacotes" ;
    rdfs:subClassOf :Metric .

# Domains
:Domain a owl:Class ;
    rdfs:comment "Domínio de rede (RAN, Transport, Core)" ;
    rdfs:subClassOf owl:Thing .

:RAN a owl:Class ;
    rdfs:comment "Radio Access Network domain" ;
    rdfs:subClassOf :Domain .

:Transport a owl:Class ;
    rdfs:comment "Transport Network domain" ;
    rdfs:subClassOf :Domain .

:Core a owl:Class ;
    rdfs:comment "Core Network domain" ;
    rdfs:subClassOf :Domain .

# Templates
:GSTTemplate a owl:Class ;
    rdfs:comment "Generic Slice Template conforme 3GPP" ;
    rdfs:subClassOf owl:Thing .

:NESTTemplate a owl:Class ;
    rdfs:comment "Network Slice Template conforme 3GPP TS 28.541" ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :generatedFromGST ;
        owl:someValuesFrom :GSTTemplate
    ] .

# Decision
:Decision a owl:Class ;
    rdfs:comment "Decisão do Decision Engine" ;
    rdfs:subClassOf owl:Thing .

:AdmissionDecision a owl:Class ;
    rdfs:comment "Decisão de admissão de slice" ;
    rdfs:subClassOf :Decision .

:ReconfigurationDecision a owl:Class ;
    rdfs:comment "Decisão de reconfiguração de slice" ;
    rdfs:subClassOf :Decision .

:RiskAssessment a owl:Class ;
    rdfs:comment "Avaliação de risco de SLA" ;
    rdfs:subClassOf owl:Thing .

# Blockchain
:SmartContract a owl:Class ;
    rdfs:comment "Smart contract para registro de SLA" ;
    rdfs:subClassOf owl:Thing .

:OnChainSLAContract a owl:Class ;
    rdfs:comment "SLA registrado on-chain" ;
    rdfs:subClassOf :SmartContract ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :registersSLA ;
        owl:someValuesFrom :SLA
    ] .

:EnforcementAction a owl:Class ;
    rdfs:comment "Ação de enforcement de SLA" ;
    rdfs:subClassOf owl:Thing .

# ML
:MLModel a owl:Class ;
    rdfs:comment "Modelo de Machine Learning" ;
    rdfs:subClassOf owl:Thing .

:Prediction a owl:Class ;
    rdfs:comment "Predição de viabilidade de SLA" ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :generatedBy ;
        owl:someValuesFrom :MLModel
    ] .

:Explanation a owl:Class ;
    rdfs:comment "Explicação de predição (XAI)" ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :explainsPrediction ;
        owl:someValuesFrom :Prediction
    ] .

# Observabilidade
:TelemetrySample a owl:Class ;
    rdfs:comment "Amostra de telemetria" ;
    rdfs:subClassOf owl:Thing .

:ObservationWindow a owl:Class ;
    rdfs:comment "Janela de observação de métricas" ;
    rdfs:subClassOf owl:Thing .

# ============================================
# PROPRIEDADES (Object Properties)
# ============================================

:hasSliceType a owl:ObjectProperty ;
    rdfs:domain :Intent ;
    rdfs:range :SliceType ;
    rdfs:comment "Relaciona intent com tipo de slice" .

:hasSLA a owl:ObjectProperty ;
    rdfs:domain :Slice ;
    rdfs:range :SLA ;
    rdfs:comment "Relaciona slice com SLA" .

:hasSLO a owl:ObjectProperty ;
    rdfs:domain :SLA ;
    rdfs:range :SLO ;
    rdfs:comment "Relaciona SLA com SLO" .

:hasSLI a owl:ObjectProperty ;
    rdfs:domain :SLO ;
    rdfs:range :SLI ;
    rdfs:comment "Relaciona SLO com SLI" .

:hasMetric a owl:ObjectProperty ;
    rdfs:domain :SLI ;
    rdfs:range :Metric ;
    rdfs:comment "Relaciona SLI com métrica" .

:belongsToSLA a owl:ObjectProperty ;
    rdfs:domain :SLO ;
    rdfs:range :SLA ;
    rdfs:comment "SLO pertence a SLA" .

:measuresSLO a owl:ObjectProperty ;
    rdfs:domain :SLI ;
    rdfs:range :SLO ;
    rdfs:comment "SLI mede SLO" .

:hasDomain a owl:ObjectProperty ;
    rdfs:domain :Slice ;
    rdfs:range :Domain ;
    rdfs:comment "Slice tem domínio" .

:generatedFromGST a owl:ObjectProperty ;
    rdfs:domain :NESTTemplate ;
    rdfs:range :GSTTemplate ;
    rdfs:comment "NEST gerado a partir de GST" .

:registersSLA a owl:ObjectProperty ;
    rdfs:domain :OnChainSLAContract ;
    rdfs:range :SLA ;
    rdfs:comment "Contrato registra SLA" .

:generatedBy a owl:ObjectProperty ;
    rdfs:domain :Prediction ;
    rdfs:range :MLModel ;
    rdfs:comment "Predição gerada por modelo ML" .

:explainsPrediction a owl:ObjectProperty ;
    rdfs:domain :Explanation ;
    rdfs:range :Prediction ;
    rdfs:comment "Explicação explica predição" .

# ============================================
# PROPRIEDADES (Data Properties)
# ============================================

:hasLatency a owl:DatatypeProperty ;
    rdfs:domain [ owl:unionOf (:Slice :SLO :Metric) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Latência máxima em milissegundos" .

:hasThroughput a owl:DatatypeProperty ;
    rdfs:domain [ owl:unionOf (:Slice :SLO :Metric) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Throughput mínimo em Mbps" .

:hasReliability a owl:DatatypeProperty ;
    rdfs:domain [ owl:unionOf (:Slice :SLO :Metric) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Confiabilidade (0-1)" .

:hasJitter a owl:DatatypeProperty ;
    rdfs:domain [ owl:unionOf (:Slice :SLO :Metric) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Jitter máximo em milissegundos" .

:hasPacketLoss a owl:DatatypeProperty ;
    rdfs:domain [ owl:unionOf (:Slice :SLO :Metric) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Perda de pacotes (0-1)" .

:hasCoverage a owl:DatatypeProperty ;
    rdfs:domain :Slice ;
    rdfs:range xsd:string ;
    rdfs:comment "Cobertura (Urban, Rural, etc.)" .

:hasMobility a owl:DatatypeProperty ;
    rdfs:domain :Slice ;
    rdfs:range xsd:string ;
    rdfs:comment "Mobilidade (Stationary, Mobile, etc.)" .

:hasDeviceDensity a owl:DatatypeProperty ;
    rdfs:domain :Slice ;
    rdfs:range xsd:float ;
    rdfs:comment "Densidade de dispositivos por km²" .

:hasSST a owl:DatatypeProperty ;
    rdfs:domain :GSTTemplate ;
    rdfs:range xsd:integer ;
    rdfs:comment "Slice/Service Type (1=eMBB, 2=URLLC, 3=mMTC)" .

:hasSD a owl:DatatypeProperty ;
    rdfs:domain :GSTTemplate ;
    rdfs:range xsd:string ;
    rdfs:comment "Slice Differentiator" .

:hasViabilityScore a owl:DatatypeProperty ;
    rdfs:domain :Prediction ;
    rdfs:range xsd:float ;
    rdfs:comment "Score de viabilidade (0-1)" .

:hasRiskLevel a owl:DatatypeProperty ;
    rdfs:domain :RiskAssessment ;
    rdfs:range xsd:string ;
    rdfs:comment "Nível de risco (low, medium, high)" .

# ============================================
# INDIVÍDUOS (Instances)
# ============================================

# Domains
:RAN_Domain a :RAN ;
    rdfs:label "RAN Domain" .

:Transport_Domain a :Transport ;
    rdfs:label "Transport Domain" .

:Core_Domain a :Core ;
    rdfs:label "Core Domain" .

# Slice Types
:eMBB_Type a :SliceType ;
    rdfs:label "eMBB" ;
    :hasLatency 50.0 ;
    :hasThroughput 1000.0 ;
    :hasReliability 0.99 .

:URLLC_Type a :SliceType ;
    rdfs:label "URLLC" ;
    :hasLatency 10.0 ;
    :hasThroughput 100.0 ;
    :hasReliability 0.99999 .

:mMTC_Type a :SliceType ;
    rdfs:label "mMTC" ;
    :hasLatency 1000.0 ;
    :hasThroughput 0.1 ;
    :hasReliability 0.9 .

# Use Case Slices
:RemoteSurgery a :UseCaseSlice ;
    rdfs:label "Remote Surgery" ;
    :hasSliceType :URLLC_Type ;
    :hasLatency 1.0 ;
    :hasReliability 0.99999 .

:XR a :UseCaseSlice ;
    rdfs:label "Extended Reality" ;
    :hasSliceType :eMBB_Type ;
    :hasLatency 20.0 ;
    :hasThroughput 500.0 .

:IoTMassive a :UseCaseSlice ;
    rdfs:label "Massive IoT" ;
    :hasSliceType :mMTC_Type ;
    :hasDeviceDensity 1000000.0 ;
    :hasReliability 0.95 .

