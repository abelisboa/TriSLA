<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/trisla/ontologies/2025/trisla#"
     xml:base="http://www.semanticweb.org/trisla/ontologies/2025/trisla"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0.0</owl:versionInfo>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TriSLA Complete Ontology - Network Slice Management with SLA Guarantees</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TriSLA Ontology</rdfs:label>
    </owl:Ontology>

    <!-- ============================================ -->
    <!-- CLASSES PRINCIPAIS -->
    <!-- ============================================ -->

    <!-- Intent -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Intent">
        <rdfs:label>Intent</rdfs:label>
        <rdfs:comment>Representa uma intenção de serviço do tenant</rdfs:comment>
    </owl:Class>

    <!-- Service -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Service">
        <rdfs:label>Service</rdfs:label>
        <rdfs:comment>Serviço de rede 5G/O-RAN</rdfs:comment>
    </owl:Class>

    <!-- GST (Generic Slice Template) -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#GST">
        <rdfs:label>Generic Slice Template</rdfs:label>
        <rdfs:comment>Template genérico de slice conforme 3GPP TS 28.541</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Service"/>
    </owl:Class>

    <!-- NEST (Network Slice Template) -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NEST">
        <rdfs:label>Network Slice Template</rdfs:label>
        <rdfs:comment>Template de network slice conforme 3GPP TS 28.541</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#GST"/>
    </owl:Class>

    <!-- Network Slice (classe raiz) -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice">
        <rdfs:label>Network Slice</rdfs:label>
        <rdfs:comment>Classe raiz para todos os tipos de network slice</rdfs:comment>
    </owl:Class>

    <!-- eMBB Slice -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#eMBBSlice">
        <rdfs:label>eMBB Slice</rdfs:label>
        <rdfs:comment>Enhanced Mobile Broadband - Slice para alta taxa de dados</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
    </owl:Class>

    <!-- URLLC Slice -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#URLLCSlice">
        <rdfs:label>URLLC Slice</rdfs:label>
        <rdfs:comment>Ultra-Reliable Low-Latency Communications - Slice para baixa latência e alta confiabilidade</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
    </owl:Class>

    <!-- mMTC Slice -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#mMTCSlice">
        <rdfs:label>mMTC Slice</rdfs:label>
        <rdfs:comment>massive Machine-Type Communications - Slice para IoT massivo</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
    </owl:Class>

    <!-- Variantes Especializadas de eMBB -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Video4KSlice">
        <rdfs:label>Video 4K Slice</rdfs:label>
        <rdfs:comment>Slice especializado para streaming de vídeo 4K</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#eMBBSlice"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#XRSlice">
        <rdfs:label>XR Slice</rdfs:label>
        <rdfs:comment>Slice especializado para Extended Reality (AR/VR)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#eMBBSlice"/>
    </owl:Class>

    <!-- Variantes Especializadas de URLLC -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#RemoteSurgerySlice">
        <rdfs:label>Remote Surgery Slice</rdfs:label>
        <rdfs:comment>Slice especializado para cirurgia remota</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#URLLCSlice"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#AutonomousVehicleSlice">
        <rdfs:label>Autonomous Vehicle Slice</rdfs:label>
        <rdfs:comment>Slice especializado para veículos autônomos</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#URLLCSlice"/>
    </owl:Class>

    <!-- Variantes Especializadas de mMTC -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#MassiveIoTSlice">
        <rdfs:label>Massive IoT Slice</rdfs:label>
        <rdfs:comment>Slice especializado para IoT massivo</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#mMTCSlice"/>
    </owl:Class>

    <!-- Subset -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset">
        <rdfs:label>Subset</rdfs:label>
        <rdfs:comment>Subset de um network slice (RAN, Transport, Core)</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#RANSubset">
        <rdfs:label>RAN Subset</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#TransportSubset">
        <rdfs:label>Transport Subset</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#CoreSubset">
        <rdfs:label>Core Subset</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset"/>
    </owl:Class>

    <!-- Domain -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Domain">
        <rdfs:label>Domain</rdfs:label>
        <rdfs:comment>Domínio da rede (RAN, Transport, Core)</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#RANDomain">
        <rdfs:label>RAN Domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Domain"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#TransportDomain">
        <rdfs:label>Transport Domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Domain"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#CoreDomain">
        <rdfs:label>Core Domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Domain"/>
    </owl:Class>

    <!-- SLA -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SLA">
        <rdfs:label>Service Level Agreement</rdfs:label>
        <rdfs:comment>Acordo de Nível de Serviço</rdfs:comment>
    </owl:Class>

    <!-- SLO -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SLO">
        <rdfs:label>Service Level Objective</rdfs:label>
        <rdfs:comment>Objetivo de Nível de Serviço</rdfs:comment>
    </owl:Class>

    <!-- KPI -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#KPI">
        <rdfs:label>Key Performance Indicator</rdfs:label>
        <rdfs:comment>Indicador Chave de Performance</rdfs:comment>
    </owl:Class>

    <!-- KQI -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#KQI">
        <rdfs:label>Key Quality Indicator</rdfs:label>
        <rdfs:comment>Indicador Chave de Qualidade</rdfs:comment>
    </owl:Class>

    <!-- Resource -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Resource">
        <rdfs:label>Resource</rdfs:label>
        <rdfs:comment>Recurso de rede (CPU, memória, banda, etc.)</rdfs:comment>
    </owl:Class>

    <!-- Decision -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Decision">
        <rdfs:label>Decision</rdfs:label>
        <rdfs:comment>Decisão do Decision Engine (ACCEPT, RENEGOTIATE, REJECT)</rdfs:comment>
    </owl:Class>

    <!-- SmartContract -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SmartContract">
        <rdfs:label>Smart Contract</rdfs:label>
        <rdfs:comment>Contrato inteligente na blockchain</rdfs:comment>
    </owl:Class>

    <!-- Risk -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Risk">
        <rdfs:label>Risk</rdfs:label>
        <rdfs:comment>Risco de violação de SLA</rdfs:comment>
    </owl:Class>

    <!-- Compliance -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Compliance">
        <rdfs:label>Compliance</rdfs:label>
        <rdfs:comment>Conformidade com SLA</rdfs:comment>
    </owl:Class>

    <!-- Metrics -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Metrics">
        <rdfs:label>Metrics</rdfs:label>
        <rdfs:comment>Métricas de rede coletadas</rdfs:comment>
    </owl:Class>

    <!-- Observability -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Observability">
        <rdfs:label>Observability</rdfs:label>
        <rdfs:comment>Observabilidade via OTLP</rdfs:comment>
    </owl:Class>

    <!-- QoS Profile -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#QoSProfile">
        <rdfs:label>QoS Profile</rdfs:label>
        <rdfs:comment>Perfil de Qualidade de Serviço</rdfs:comment>
    </owl:Class>

    <!-- Slice Requirement -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement">
        <rdfs:label>Slice Requirement</rdfs:label>
        <rdfs:comment>Requisito de slice (latência, throughput, etc.)</rdfs:comment>
    </owl:Class>

    <!-- Resource Allocation -->
    <owl:Class rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#ResourceAllocation">
        <rdfs:label>Resource Allocation</rdfs:label>
        <rdfs:comment>Alocação de recursos</rdfs:comment>
    </owl:Class>

    <!-- ============================================ -->
    <!-- PROPRIEDADES DE DADOS (Data Properties) -->
    <!-- ============================================ -->

    <!-- Latency -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasLatency">
        <rdfs:label>hasLatency</rdfs:label>
        <rdfs:comment>Latência máxima em milissegundos</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Throughput -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasThroughput">
        <rdfs:label>hasThroughput</rdfs:label>
        <rdfs:comment>Throughput mínimo em Mbps</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Reliability -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasReliability">
        <rdfs:label>hasReliability</rdfs:label>
        <rdfs:comment>Confiabilidade (0.0 a 1.0)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Jitter -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasJitter">
        <rdfs:label>hasJitter</rdfs:label>
        <rdfs:comment>Jitter máximo em milissegundos</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Packet Loss -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasPacketLoss">
        <rdfs:label>hasPacketLoss</rdfs:label>
        <rdfs:comment>Taxa de perda de pacotes (0.0 a 1.0)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Coverage -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasCoverage">
        <rdfs:label>hasCoverage</rdfs:label>
        <rdfs:comment>Tipo de cobertura (Urban, Rural, Suburban)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Mobility -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasMobility">
        <rdfs:label>hasMobility</rdfs:label>
        <rdfs:comment>Nível de mobilidade (Stationary, Low, Medium, High)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Device Density -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasDeviceDensity">
        <rdfs:label>hasDeviceDensity</rdfs:label>
        <rdfs:comment>Densidade de dispositivos por km²</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- Availability -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasAvailability">
        <rdfs:label>hasAvailability</rdfs:label>
        <rdfs:comment>Disponibilidade (0.0 a 1.0)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Isolation -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasIsolation">
        <rdfs:label>hasIsolation</rdfs:label>
        <rdfs:comment>Nível de isolamento (None, Partial, Full)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- SST (Slice/Service Type) -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasSST">
        <rdfs:label>hasSST</rdfs:label>
        <rdfs:comment>SST conforme 3GPP (1=eMBB, 2=URLLC, 3=mMTC)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#GST"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- SD (Slice Differentiator) -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasSD">
        <rdfs:label>hasSD</rdfs:label>
        <rdfs:comment>SD conforme 3GPP</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#GST"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- ============================================ -->
    <!-- PROPRIEDADES DE OBJETO (Object Properties) -->
    <!-- ============================================ -->

    <!-- hasRequirement -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasRequirement">
        <rdfs:label>hasRequirement</rdfs:label>
        <rdfs:comment>Relaciona slice com seus requisitos</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SliceRequirement"/>
    </owl:ObjectProperty>

    <!-- hasSubset -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasSubset">
        <rdfs:label>hasSubset</rdfs:label>
        <rdfs:comment>Relaciona NEST com seus subsets</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NEST"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset"/>
    </owl:ObjectProperty>

    <!-- hasDomain -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasDomain">
        <rdfs:label>hasDomain</rdfs:label>
        <rdfs:comment>Relaciona subset com seu domínio</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Subset"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Domain"/>
    </owl:ObjectProperty>

    <!-- hasSLA -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasSLA">
        <rdfs:label>hasSLA</rdfs:label>
        <rdfs:comment>Relaciona serviço com seu SLA</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Service"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SLA"/>
    </owl:ObjectProperty>

    <!-- hasSLO -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasSLO">
        <rdfs:label>hasSLO</rdfs:label>
        <rdfs:comment>Relaciona SLA com seus SLOs</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SLA"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#SLO"/>
    </owl:ObjectProperty>

    <!-- generatesNEST -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#generatesNEST">
        <rdfs:label>generatesNEST</rdfs:label>
        <rdfs:comment>Relaciona Intent com NEST gerado</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Intent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NEST"/>
    </owl:ObjectProperty>

    <!-- generatesGST -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#generatesGST">
        <rdfs:label>generatesGST</rdfs:label>
        <rdfs:comment>Relaciona Intent com GST gerado</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Intent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#GST"/>
    </owl:ObjectProperty>

    <!-- hasResource -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasResource">
        <rdfs:label>hasResource</rdfs:label>
        <rdfs:comment>Relaciona slice com recursos alocados</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#Resource"/>
    </owl:ObjectProperty>

    <!-- hasQoSProfile -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trisla/ontologies/2025/trisla#hasQoSProfile">
        <rdfs:label>hasQoSProfile</rdfs:label>
        <rdfs:comment>Relaciona slice com perfil de QoS</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trisla/ontologies/2025/trisla#NetworkSlice"/>
